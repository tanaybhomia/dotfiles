      "shift shift": "file_finder::Toggle",
      "ctrl-shift-backspace": "editor::DeleteLine",
      "alt-right": "workspace::ActivateNextPane",
      "alt-left": "workspace::ActivatePreviousPane"
    }
  },
  {
    "context": "Editor",
